    "keywords": [
        "phpunit",
        "xhprof"
    ],
    "homepage": "https://github.com/LightAir/phpunit-testlistener-xhprof/",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sebastian Bergmann",
            "email": "sb@sebastian-bergmann.de",
            "role": "lead"
        },
        {
            "name": "Benjamin Eberlei",
            "email": "kontakt@beberlei.de",
            "role": "lead"
        }
    ],
    "support": {
        "issues": "https://github.com/LightAir/phpunit-testlistener-xhprof/issues"
    },
    "require": {
        "php": ">=5.6",
        "phpunit/phpunit": "5.6.*"
    },
    "autoload": {
